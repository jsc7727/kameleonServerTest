{
  "name": "omega-consumer-backend",
  "version": "1.0.0",
  "description": "omega-consumer-backend",
  "main": "index.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "cross-env NODE_ENV=production tsc --project tsconfig.json",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "startPro": "cross-env NODE_ENV=production pm2 start ./build/index.js",
    "startProDev": "cross-env NODE_ENV=production NODE_ENV_DEV=true nodemon ./index.ts",
    "pro": "cross-env NODE_ENV=production tsc --project tsconfig.json && cross-env NODE_ENV=production node ./dist/index.js",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "dockerBuild": "docker build . -t omega",
    "dockerRun": "docker run -p 7777:7777 -t omega",
    "test": "jest --setupFiles dotenv/config --forceExit --detectOpenHandles",
    "migration": "yarn typeorm migration:run"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "caver-js": "^1.8.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "file": "^0.2.2",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.1.4",
    "jest": "^27.4.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql": "^2.18.1",
    "path": "^0.12.7",
    "supertest": "^6.1.6",
    "typeorm": "^0.2.38",
    "typeorm-extension": "^1.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.6",
    "@types/styled-components": "^5.1.19",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "eslint": "7.32.0",
    "eslint-config-next": "^11.1.2",
    "jslint": "^0.12.1",
    "nodemon": "^2.0.14",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StartPlayUp/omega-consumer-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StartPlayUp/omega-consumer-backend/issues"
  },
  "homepage": "https://github.com/StartPlayUp/omega-consumer-backend#readme"
}
